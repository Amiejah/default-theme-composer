{
    "name": "humpff/default-wp-theme",
    "type": "wordpress-theme",
    "description": "The Default WordPress theme managed via Composer.",
    "homepage": "https://github.com/Amiejah/default-theme-composer.git",
    "authors": [{
			  "name": "Humpff",
			  "homepage": "https://humpff.com"
		}],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^11.36.1",
        "illuminate/collections": "^11.36",
        "illuminate/support": "^11.36",
        "htmlburger/carbon-fields": "^3.6"
    },
    "autoload": {
      "psr-4": {
        "Humpff\\": "App/"
      }
    },
    "scripts": {
      "lint": "phpcs",
      "format": "phpcbf"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": ">=3.11.2",
      "wp-coding-standards/wpcs": "^3.1.0",
      "symfony/var-dumper": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
